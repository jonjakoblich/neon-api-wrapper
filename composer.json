{
    "name": "jonjakoblich/neon-api-wrapper",
    "description": "A PHP wrapper for the Neon CRM API.",
    "version": "0.1",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.3",
        "guzzlehttp/guzzle": "^7.9"
    },
    "require-dev": {
        "pestphp/pest": "^3.4",
        "phpstan/phpstan": "^1.12",
        "symfony/var-dumper": "^3.4",
        "fakerphp/faker": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "TwoJays\\NeonApiWrapper\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
